{
  "name": "trading-assistant",
  "description": "AI-powered trading assistant with smart money concepts",
  "type": "web",
  "build": {
    "command": "pip install -r requirements.txt"
  },
  "start": {
    "command": "python run.py"
  },
  "env": {
    "PORT": "5000"
  },
  "healthcheck": {
    "path": "/api/market-data/GOLD",
    "interval": 30
  }
} 